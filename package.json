{
  "name": "NodeAPI",
  "version": "1.0.0",
  "author": "Evgeny Aleksandrov",
  "description": "Simple example of RESTful API with Node.js and passport",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "test": "nodemon ./bin/www --mock"
  },
  "dependencies": {
    "body-parser": "~1.8.x",
    "braintree": "^1.37.1",
    "cookie-parser": "~1.3.x",
    "debug": "~2.0.x",
    "express": "~4.10.x",
    "faker": "^2.0.x",
    "lodash": "^4.7.0",
    "method-override": "^2.3.x",
    "mongoose": "^3.8.x",
    "nconf": "^0.6.x",
    "oauth2orize": "^1.0.x",
    "oauthio": "^0.3.3",
    "passport": "^0.2.x",
    "passport-http": "^0.2.x",
    "passport-http-bearer": "^1.0.x",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "passport-local-mongoose-email": "^0.1.2",
    "passport-oauth": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.x",
    "request": "^2.70.0",
    "winston": "^0.8.x"
